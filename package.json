{
  "name": "siyuan-plugin-simplemindmap",
  "version": "0.5.5",
  "type": "module",
  "description": "A mind map plugin for Siyuan Notes using simple-mind-map",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development VITE_SOURCEMAP=inline vite build --watch",
    "release": "node ./release.js",
    "release:manual": "node ./release.js --mode=manual",
    "release:patch": "node ./release.js --mode=patch",
    "release:minor": "node ./release.js --mode=minor",
    "release:major": "node ./release.js --mode=major",
    "build": "cross-env NODE_ENV=production vite build",
    "make_dev_copy": "node --no-warnings ./scripts/make_dev_copy.js"
  },
  "dependencies": {
    "simple-mind-map": "file:mind-map/simple-mind-map"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^4.0.1",
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^20.2.0",
    "cross-env": "^7.0.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "1.1.6",
    "svelte": "^4.2.19",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^5.2.9",
    "vite-plugin-static-copy": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "pnpm": {
    "overrides": {
      "rollup": "4.52.5"
    }
  }
}
